

<% if current_user.likes?(@tweet) %>
  <%= link_to "UnLike", tweet_like_path(@tweet), method: :delete %>
<% else %>
  <%= link_to "Like", tweet_like_path(@tweet), method: :post %>
<% end %>

<%= @tweet.likes.count %><br>

<h3 class="">Liked By:</h3>
<% @tweet.likes.each do |like| %>
<%= like.user.username %><br>
<% end %>
